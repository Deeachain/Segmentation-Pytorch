ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=psamask_cpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /home/ding/anaconda3/envs/segment/lib/python3.6/site-packages/torch/include -isystem /home/ding/anaconda3/envs/segment/lib/python3.6/site-packages/torch/include/torch/csrc/api/include -isystem /home/ding/anaconda3/envs/segment/lib/python3.6/site-packages/torch/include/TH -isystem /home/ding/anaconda3/envs/segment/lib/python3.6/site-packages/torch/include/THC -isystem /home/ding/anaconda3/envs/segment/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++11
ldflags = -shared

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

build operator.o: compile /media/ding/Study/graduate/code/Efficient-Segmentation-Networks/model/PSPNet/lib/psa/src/cpu/operator.cpp
build psamask.o: compile /media/ding/Study/graduate/code/Efficient-Segmentation-Networks/model/PSPNet/lib/psa/src/cpu/psamask.cpp

build psamask_cpu.so: link operator.o psamask.o

default psamask_cpu.so

